{
    "name": "andreascandle/tekenaja",
    "description": "Packages for using teken aja API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "http",
        "rest",
        "php",
        "TekenAja",
        "TekenAja PHP",
        "Digital Signature Library",
        "HTTP client"
    ],
    "autoload": {
        "psr-4": {
            "AndreasCandle\\TekenAja\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andreas Panjaitan",
            "email":"andreas@singa.id"
        },
        {
            "name": "Andreas Panjaitan",
            "email": "andreaspanjaitan89@gmail.com"
        },
        {
            "name": "Andreas Panjaitan",
            "email": "andreascandle89@gmail.com"
        }
    ],
    "require": {
        "vlucas/phpdotenv": "5.5.x-dev",
        "mashape/unirest-php": "3.0.4",
        "ext-curl": "*"
    },
    "config": {
        "sort-packages": true
      },
      "prefer-stable": true,
      "minimum-stability": "dev",
      "scripts": {
        "test": "phpunit --colors=always",
        "test:ci": "composer test -- --verbose --coverage-text --coverage-clover=coverage.xml"
      },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
